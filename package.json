{
  "name": "p2o",
  "version": "0.0.1",
  "description": "Convert postman collection json to OpenAPI yaml documents",
  "main": "./src/index.js",
  "bin": {
    "p2o": "./lib/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "test": "mocha"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcodelabs/p2o.git"
  },
  "keywords": [
    "p2o",
    "postman",
    "openapi",
    "nodejs",
    "convert",
    "transform",
    "cli"
  ],
  "author": "Fcode Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fcodelabs/p2o/issues"
  },
  "homepage": "https://github.com/fcodelabs/p2o#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^7.16.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3"
  }
}
